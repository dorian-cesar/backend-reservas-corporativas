{
  "name": "backend-reservas-corporativas",
  "version": "1.0.0",
  "description": "Backend multiempresa para reservas corporativas Pullman Bus - WIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "seed:superuser": "ts-node script/seedSuperUser.ts",
    "syncDB": "ts-node src/script/syncDB.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch"
  },
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.3",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.39.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
