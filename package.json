{
  "name": "backend-reservas-corporativas",
  "version": "1.0.0",
  "description": "Backend multiempresa para reservas corporativas Pullman Bus - WIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "seed:superuser": "ts-node src/script/seedSuperUser.ts",
    "syncDB": "ts-node src/script/syncDB.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "populateTicket": "ts-node src/script/populateEmpresaInTickets.ts",
    "generarEstados": "ts-node src/cron/ticketsFacturacionActual.ts",
    "update-centros": "ts-node src/script/update-centros-costo-from-csv.ts",
    "generar-cuentas-corrientes": "ts-node src/script/generarCuentasCorrientes.ts",
    "generar-estado-cuenta": "ts-node src/script/generarEDPManual.ts",
    "tickets-facturacion-actual": "ts-node src/cron/ticketsFacturacionActual.ts",
    "recalcular-edp": "ts-node src/script/recalcularEstadosCuenta.ts"
  },
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "bcrypt": "^5.1.0",
    "bwip-js": "^4.8.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "pdf-lib": "^1.17.1",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.1",
    "jest": "^29.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}